{
  "name": "pluralizer",
  "version": "1.0.0",
  "description": "A library for pluralizing words in English and Russian",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test:unit": "vitest run --dir tests/unit",
    "test:e2e": "vitest run --dir tests/e2e",
    "test": "vitest run",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "keywords": [
    "pluralize",
    "pluralization",
    "english",
    "russian",
    "i18n"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "rollup": "^4.35.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  }
}
